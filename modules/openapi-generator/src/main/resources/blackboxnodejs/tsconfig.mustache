{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "es2015", "dom"],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*",
        "gensrc/*"
      ]
    },
  }
}
